local SettingsTab = TabSection:CreateTab({
    Name = "Settings",
    Icon = NebulaIcons:GetIcon('settings', 'Material'),
    Columns = 1
}, "INDEX")

local SettingsSection = SettingsTab:CreateSection("Settings")
SettingsSection:CreateKeybind({
    Name = "Toggle UI",
    CurrentKeybind = "K",
    HoldToInteract = false,
    Callback = function()
        Starlight:ToggleUI()
    end
})
