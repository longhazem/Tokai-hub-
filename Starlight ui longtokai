local Starlight = loadstring(game:HttpGet("https://raw.nebulasoftworks.xyz/starlight"))()
local NebulaIcons = loadstring(game:HttpGet("https://raw.nebulasoftworks.xyz/nebula-icon-library-loader"))()

local Window = Starlight:CreateWindow({
    Name = "Tokai-hub",
    Subtitle = "v1.0",
    Icon = 123456789,
    LoadingSettings = {
        Title = "Toka-Hub",
        Subtitle = "Welcome to Tokai Hub",
    },
    FileSettings = {
        ConfigFolder = "Tokai-hub"
    },
})

Window:PromptDialog({
    Name = "info",
    Content = "Welcome to ToKai Hub, enjoy your experience!.",
    Type = 1,
    Actions = { 
        Primary = {
            Name = "info!",
            Callback = function()
                print("Okay!")
            end
        },
        {
            Name = "Cancel",
            Callback = function()
                print("Cancel!")
            end
        },
    }
})

local TabSection = Window:CreateTabSection("MAIN")

