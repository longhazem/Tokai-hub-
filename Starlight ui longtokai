local Starlight = loadstring(game:HttpGet("https://raw.nebulasoftworks.xyz/starlight"))()
local NebulaIcons = loadstring(game:HttpGet("https://raw.nebulasoftworks.xyz/nebula-icon-library-loader"))()

local Window = Starlight:CreateWindow({
    Name = "TOKAI-HUB-Zaibatsu+RP",
    Subtitle = "by LONGTOKAI",
    Icon = 0,
    Theme = "Bloom",
    LoadingSettings = {
        Title = "TOKAI",
        Subtitle = "Initializing..."
    },
    FileSettings = {
        ConfigFolder = "TOKAI-HUB",
        FileName = "Big Hub"
    }
})

local TabSection = Window:CreateTabSection("Main TabSection", true)

local function teleportTo(x, y, z)
    local p = game.Players.LocalPlayer
    if p and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
        p.Character.HumanoidRootPart.CFrame = CFrame.new(x, y, z)
    end
end

local MainTab = TabSection:CreateTab({
    Name = "Main",
    Icon = NebulaIcons:GetIcon('home', 'Material'),
    Columns = 1
}, "INDEX")

local MainSection = MainTab:CreateSection("Section Example")

local MiscTab = TabSection:CreateTab({
    Name = "Misc",
    Icon = NebulaIcons:GetIcon('widgets', 'Material'),
    Columns = 1
}, "INDEX")

local MiscSection = MiscTab:CreateSection("Scripts")
MiscSection:CreateButton({
    Name = "emote7yd7",
    Callback = function()
        loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-7yd7-I-Emote-Script-48024"))()
    end
})
MiscSection:CreateButton({
    Name = "IY",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end
})
MiscSection:CreateButton({
    Name = "Invisible",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostPlayer352/Test4/refs/heads/main/Invisible%20Mode"))()
    end
})

